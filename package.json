{
	"name": "gatsby-web",
	"version": "1.0.0",
	"description": "Personal portfolio website built with Gatsby, TypeScript, and Tailwind CSS",
	"main": "index.js",
	"scripts": {
		"dev": "gatsby develop",
		"start": "gatsby develop",
		"build": "gatsby build",
		"build:htdocs": "gatsby build --prefix-paths && node scripts/build-htdocs.js",
		"serve": "gatsby serve",
		"serve:htdocs": "npx serve htdocs -p 8080",
		"clean": "gatsby clean",
		"type-check": "tsc --noEmit"
	},
	"keywords": [
		"gatsby",
		"typescript",
		"tailwindcss",
		"portfolio"
	],
	"author": "Kouta Ozawa",
	"license": "MIT",
	"dependencies": {
		"@anthropic-ai/claude-code": "^2.0.8",
		"gatsby": "^5.15.0",
		"gatsby-plugin-image": "^3.15.0",
		"gatsby-plugin-manifest": "^5.15.0",
		"gatsby-plugin-sharp": "^5.15.0",
		"gatsby-source-filesystem": "^5.15.0",
		"gatsby-transformer-sharp": "^5.15.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"autoprefixer": "^10.0.0",
		"gatsby-plugin-postcss": "^6.15.0",
		"gatsby-plugin-typescript": "^5.15.0",
		"postcss": "^8.5.6",
		"tailwindcss": "^3.0.0",
		"typescript": "^5.9.3"
	}
}
